{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/upload",
      "dest": "index.js"
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/profile/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/subscribe/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/patent/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/interaction/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
